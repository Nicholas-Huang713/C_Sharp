@{
    ViewData["Title"] = "Home Page";
}
<div class="container-fluid">
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">FishON!</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" href="/registration">Register</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" href="/login">Login</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
  <ol class="carousel-indicators">
    <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
    <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
    <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
  </ol>
  <div class="carousel-inner">
    <div class="carousel-item active">
        <img class="w-100" src="https://images.unsplash.com/photo-1493787039806-2edcbe808750?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&w=1000&q=80" alt="First slide">
        <div class="carousel-caption d-none d-md-block">
        <h1>Get Hooked!</h1>
        <p>Experience Fishing in a whole new way</p>
        </div>
    </div>
    <div class="carousel-item">
        <img class=" w-100" src="https://media.istockphoto.com/photos/fishing-on-the-lake-at-sunset-fishing-background-picture-id942157916?k=6&m=942157916&s=612x612&w=0&h=gLLmn8K57jVN1nI7RcY1m3VpKZaT2_lKCEc2cCyJCAA=" alt="Second slide">
        <div class="carousel-caption d-none d-md-block">
        <h5>Stay Connected!</h5>
        <p>Stay in touch with your fellow anglers</p>
        </div>
    </div>
    <div class="carousel-item">
        <img class="w-100" src="https://media.istockphoto.com/photos/fly-fishing-in-winter-at-sunrise-picture-id638712014?k=6&m=638712014&s=612x612&w=0&h=alSUfRVJumay3cx0lW76MqKgATIF-YssLbjBbvehIoY=" alt="Third slide">
        <div class="carousel-caption d-none d-md-block">
        <h5>Find the Best Spots!</h5>
        <p>See where others are catching fish</p>
        </div>
    </div>
    </div>
        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>

</div>


